{
	"name": "pinx-nuxt-kit",
	"type": "module",
	"version": "1.20.0",
	"private": true,
	"engines": {
		"node": ">=18.0.0"
	},
	"scripts": {
		"dev": "nuxt dev",
		"dev:host": "nuxt --host",
		"build": "nuxt build",
		"build:check": "run-s libs:reload build lint type-check format test:e2e test:unit",
		"generate": "nuxt generate",
		"preview": "nuxt preview",
		"postinstall": "nuxt prepare",
		"test:unit": "vitest run",
		"test:e2e": "start-server-and-test preview http://localhost:3000/ 'cypress run --e2e'",
		"test:e2e:dev": "start-server-and-test preview http://localhost:3000/ 'cypress open --e2e'",
		"type-check": "vue-tsc --build --force",
		"lint": "eslint . --fix",
		"format": "prettier --write .",
		"lint-type-format": "run-s lint type-check format",
		"design-tokens": "node scripts/tokens-tool.js",
		"libs:check": "taze",
		"libs:reload": "rm -rf node_modules package-lock.json && npm install",
		"prepare": "husky"
	},
	"dependencies": {
		"@fontsource/jetbrains-mono": "^5.2.5",
		"@fontsource/lexend": "^5.2.7",
		"@fontsource/public-sans": "^5.2.5",
		"@popperjs/core": "^2.11.8",
		"@vueuse/components": "^13.2.0",
		"@vueuse/core": "^13.2.0",
		"colord": "^2.9.3",
		"dayjs": "^1.11.13",
		"highlight.js": "^11.11.1",
		"lodash": "^4.17.21",
		"mitt": "^3.0.1",
		"naive-ui": "^2.41.0",
		"pinia": "^3.0.2",
		"pinia-plugin-persistedstate": "^4.3.0",
		"validator": "^13.15.0",
		"vue": "^3.5.14",
		"vue-advanced-cropper": "^2.8.9",
		"vue-highlight-words": "^3.0.1",
		"vue-router": "^4.5.1"
	},
	"optionalDependencies": {
		"treemate": "^0.3.11",
		"vueuc": "^0.4.64"
	},
	"devDependencies": {
		"@antfu/eslint-config": "^4.13.1",
		"@clack/prompts": "^0.10.1",
		"@commitlint/cli": "^19.8.1",
		"@commitlint/config-conventional": "^19.8.1",
		"@css-render/vue3-ssr": "^0.15.14",
		"@iconify/vue": "^5.0.0",
		"@nuxt/devtools": "^2.4.1",
		"@nuxt/eslint": "^1.4.1",
		"@nuxt/test-utils": "^3.19.0",
		"@nuxtjs/device": "^3.2.4",
		"@nuxtjs/i18n": "^9.5.4",
		"@pinia/nuxt": "^0.11.0",
		"@tailwindcss/vite": "^4.1.7",
		"@types/dom-view-transitions": "^1.0.6",
		"@types/fs-extra": "^11.0.4",
		"@types/inquirer": "^9.0.8",
		"@types/jsdom": "^21.1.7",
		"@types/lodash": "^4.17.17",
		"@types/node": "^22.15.20",
		"@types/validator": "^13.15.1",
		"@vue/test-utils": "^2.4.6",
		"@vue/tsconfig": "^0.7.0",
		"cypress": "^14.4.0",
		"eslint": "^9.27.0",
		"fs-extra": "^11.3.0",
		"husky": "^9.1.7",
		"jsdom": "^26.1.0",
		"npm-run-all2": "^8.0.3",
		"nuxt": "^3.17.4",
		"nuxt-svgo": "^4.2.0",
		"nuxtjs-naive-ui": "^1.0.2",
		"prettier": "^3.5.3",
		"prettier-plugin-tailwindcss": "^0.6.11",
		"sass": "^1.89.0",
		"start-server-and-test": "^2.0.12",
		"tailwindcss": "^4.1.7",
		"taze": "^19.1.0",
		"ts-node": "^10.9.2",
		"type-fest": "^4.41.0",
		"typescript": "~5.8.3",
		"vitest": "^3.1.4",
		"vue-tsc": "^2.2.10"
	},
	"pnpm": {
		"overrides": {
			"vite-plugin-checker>vue-tsc": "$vue-tsc"
		},
		"onlyBuiltDependencies": [
			"@parcel/watcher",
			"@tailwindcss/oxide",
			"cypress",
			"esbuild",
			"unrs-resolver"
		]
	},
	"overrides": {
		"@nuxt/test-utils": {
			"happy-dom": "15.0.0"
		}
	},
	"resolutions": {
		"@typescript-eslint/typescript-estree": "^8.26.1"
	}
}
